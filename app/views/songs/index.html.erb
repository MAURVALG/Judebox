<h1>Songs List</h1>

<table border="1",  class="table">
	<tr>
		<th> Name </th>
		<th> Artist </th>
		<th> Album </th>
		<th> Song </th>
		<th> Options </th>
		
		


	</tr>
	<% @songs.each do |s| %>
		<tr>
			<td> <%= s.song_name %> </td>
			<td><%= s.artist_name %></td>
			<td><%= s.album_name %></td>
			<td><audio src="<%= s.attachment_url %>" type="audio/mpeg" controls></audio></td>
			<td><%= link_to "Edit" , edit_song_path(s) %> |
				<%= link_to "Show" , song_path(s) %>|
				<%= link_to "Delete" , song_path(s), :confirm => "Â¿Estas eguro?", :method => :delete%>
			</td>
			
		</tr>
					
		<% end %>
	</table>

<br>
<button><%= link_to "New" , new_song_path %></button>
<button><%= link_to "Back" , :controller => "home", :accion => "index" %></button>
<button><%= link_to "Salir", destroy_user_session_path, method: :delete %></button>
